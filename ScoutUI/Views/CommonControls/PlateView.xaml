<UserControl x:Class="ScoutUI.Views.CellType.UserControls.PlateView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ucCommon="clr-namespace:ScoutUI.Views.ucCommon"
             mc:Ignorable="d">
    <Grid>
        <Grid>
            <ucCommon:PlateUserControl MyItemsSource="{Binding QueueManagementModel.PlateSamples}"
                IsRowWiseColumnWise="{Binding QueueManagementModel.IsRowWisePosition}"
                x:Name="ucCarouselGrid" />
        </Grid>
    </Grid>
</UserControl>